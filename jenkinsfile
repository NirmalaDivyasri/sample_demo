pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Checkout Code')
        {
            steps {
                git 'https://github.com/NirmalaDivyasri/sample_demo.git'
            }
        }
        stage('Run unit test cases')
        {
            steps {
                echo 'run clean tests'
            }
        }
    }
}
